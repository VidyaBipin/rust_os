{
	"cpu": "cortex-a57",
	"data-layout": "e-m:e-i8:8:32-i16:16:32-i64:64-i128:128-n32:64-S128",
	"llvm-target": "aarch64-none-elf",
	"target-endian": "little",
	"target-pointer-width": "64",
	"target-word-size": "64",
	"target-c-int-width": "32",
	"os": "tifflin",
	"arch": "aarch64",
		"linker-flavor": "ld",
		"linker": "aarch64-none-elf-ld",
		"pre-link-args": {"ld": ["-T","link-armv8.ld", "-z","max-page-size=0x4000", "--gc-sections", "--start-group"] },
		"post-link-args": {"ld": ["--end-group"]},
		"no-compiler-rt": true,
		"features": "-neon,-fp-armv8", "_comment": "TODO: Enable all the fun features, but that requires kernel suppport",
		"executables": true,
		"dynamic-linking": true,
	"morestack": false
}
